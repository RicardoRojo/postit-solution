<%= render "shared/errors", error_obj: @post %>
<%= form_for @post do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :description %>
  <%= f.text_area :description, rows: 5 %> <br />
  <%= f.submit(@post.new_record? ? "Create post" : "Update post" , class: "btn btn-success") %>
<% end %>